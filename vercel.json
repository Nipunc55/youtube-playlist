{
  "rewrites": [
    {
      "source": "/blog/:path*",
      "destination": "https://kalanawijewardhana.shop/:path*"
    },
    {
      "source": "/assets/*",
      "destination": "https://kalanawijewardhana.shop/assets/:splat"
    },
    {
      "source": "/content/*",
      "destination": "https://kalanawijewardhana.shop/content/:splat"
    },
    {
      "source": "/public/*",
      "destination": "https://kalanawijewardhana.shop/public/:splat"
    },
    {
      "source": "/blog/:post_slug*",
      "destination": "https://kalanawijewardhana.shop/:post_slug*"
    }
  ]
}
